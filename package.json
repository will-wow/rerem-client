    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^24.9.0",
    "jest-transform-svelte": "https://github.com/will-wow/jest-transform-svelte.git",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "prettier-plugin-svelte": "^0.7.0",
