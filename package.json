    "@types/node-forge": "^0.8.6",
    "@types/page": "^1.8.0",
    "@types/query-string": "^6.3.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.2.1",
    "humps": "^2.0.1",
    "node-forge": "^0.8.5",
    "page": "^1.11.4",
    "query-string": "^6.8.2",
    "regenerator-runtime": "^0.13.3",
    "result-async": "^1.2.4",
    "sirv-cli": "^0.4.4"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "format": "npm-run-all -c format:*",
    "format:eslint": "eslint --fix '**/*.{js,ts,svelte}'",
    "format:prettier": "prettier --write '**/*.{js,ts,svelte,json,md,css}'",
    "lint": "npm-run-all -c lint:*",
    "format:types": "tsc",
    "lint:eslint": "eslint '**/*.{js,ts,svelte}'",
    "lint:prettier": "prettier --check '**/*.{js,ts,svelte,json,md,css}'",
    "lint:types": "tsc",
    "test": "jest",
    "deploy": "npm-run-all lint build deploy-surge",
    "deploy-surge": "surge public --domain https://rerem.surge.sh"
  }
}
